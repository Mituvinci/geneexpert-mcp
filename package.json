{
  "name": "geneexpert-mcp",
  "version": "0.1.0",
  "description": "Multi-agent MCP server for bioinformatics with Claude, GPT-4, and Gemini collaboration",
  "main": "src/mcp/server.js",
  "type": "module",
  "bin": {
    "geneexpert": "./bin/geneexpert.js"
  },
  "scripts": {
    "start": "node src/mcp/server.js",
