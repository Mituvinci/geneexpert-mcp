  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "glob": "^10.5.0",
    "openai": "^4.77.0"
  }
}
